{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:clean": "rm -rf .nuxt .output dist && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "perf:audit": "npx lighthouse http://localhost --output=html --output-path=./reports/lighthouse.html",
    "perf:auditjson": "npx lighthouse http://localhost --output=json --output-path=./reports/lighthouse.json",
    "perf:bundle": "npx nuxi analyze",
    "docker:frontend": "docker-compose -f docker-compose.frontend.yml up --build",
    "docker:frontend:down": "docker-compose -f docker-compose.frontend.yml down",
    "docker:original": "docker-compose up --build",
    "docker:original:down": "docker-compose down",
    "deploy:vercel": "./scripts/deploy-vercel.sh",
    "deploy:vercel:preview": "./scripts/deploy-vercel.sh preview",
    "deploy:vercel:production": "./scripts/deploy-vercel.sh production",
    "vercel:env": "vercel env ls",
    "vercel:logs": "vercel logs",
    "vercel:domains": "vercel domains ls"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@iconify-json/carbon": "^1.2.10",
    "@nuxt/icon": "1.15.0",
    "@nuxt/image": "1.10.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^10.0.0",
    "@nuxtjs/seo": "^3.1.0",
    "@pinia/nuxt": "^0.11.1",
    "@sidebase/nuxt-auth": "^1.0.0",
    "@unocss/reset": "^66.3.3",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "nuxt": "^3.17.6",
    "pinia": "^3.0.3",
    "rate-limiter-flexible": "^7.1.1",
    "unocss": "^0.62.4",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.6.0",
    "@nuxt/test-utils": "3.19.2",
    "@unocss/nuxt": "^0.62.4",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.31.0",
    "happy-dom": "^18.0.1",
    "typescript": "^5.8.3",
    "unplugin-turbo-console": "^1.0.2",
    "vitest": "^3.2.4"
  }
}
